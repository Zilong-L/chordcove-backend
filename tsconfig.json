{
    "include": ["src/**/*", "worker-configuration.d.ts"],
    "exclude": ["functions/**/*"],
    "compilerOptions": {
        "allowJs": true,
        "outDir": "./dist",
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "esnext"
        ],
        "types": ["./.wrangler/types/runtime"],
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true
    }
}